generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Roles {
  ADMIN
  MEMBER
}

model User {
  id            String @id @default(uuid())
  name          String
  email         String @unique
  password_hash String
  cargo         String
  role          Roles  @default(MEMBER)
}
